<h1>Campus Explorer Data</h1>
<%= link_to ces_path(format: :csv), class: 'left' do %>
  Export
  <i class="material-icons">grid_on</i>
<% end %>
<table>
  <thead>
      <% Ce.cols.each do |col| %>
       <th><%= col -%> </th>
      <% end %>
  </thead>

  <tbody>
    <% @ces.each do |ce| %>
      <tr>
         <% Ce.cols.each do |col| %>
           <td><%= ce.send(col) -%></td>
         <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate(@ces) -%>
