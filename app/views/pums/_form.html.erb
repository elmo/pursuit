
<%= simple_form_for(@pum) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :date %>
    <%= f.input :label %>
    <%= f.input :account_id %>
    <%= f.input :campaign_id %>
    <%= f.input :ad_group_id %>
    <%= f.input :keyword_id %>
    <%= f.input :publisher %>
    <%= f.input :account_name %>
    <%= f.input :campaign_name %>
    <%= f.input :ad_group_name %>
    <%= f.input :keyword %>
    <%= f.input :focus_word %>
    <%= f.input :industry %>
    <%= f.input :device_type %>
    <%= f.input :impressions %>
    <%= f.input :cost_per_click %>
    <%= f.input :cost %>
    <%= f.input :click_count %>
    <%= f.input :total_unreconciled_revenue %>
    <%= f.input :total_clickout_revenue %>
    <%= f.input :leads %>
    <%= f.input :clickouts %>
    <%= f.input :lead_users %>
    <%= f.input :lead_request_users %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
